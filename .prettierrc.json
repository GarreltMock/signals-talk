    "printWidth": 120,
    "semi": false,
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
        {
            "files": ["slides.md", "pages/*.md"],
            "options": {
                "parser": "slidev",
                "plugins": ["prettier-plugin-slidev"]
            }
        }
    ]
}
